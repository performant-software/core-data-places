{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": [
      "vite/client"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "allowJs": true
  }
}