---
import Container from '../../../components/Container.astro';
import { TinaMarkdown } from 'tinacms/dist/rich-text';

const { content } = Astro.props;
---

<Container
  className='w-full py-8'
>
  <article
    class='prose prose-lg max-w-none w-full'
  >
    <TinaMarkdown
      content={content}
    />
  </article>
</Container>