---
import Search from '@apps/search/Search';
import { getTranslations } from '@i18n/server';
import Layout from "@layouts/Layout.astro";

const { t } = await getTranslations(Astro.currentLocale);
---

<Layout
  fullscreen
  t={t}
  title={t('map')}
  tab="map"
>
  <Search
    client:only="react"
  />
</Layout>
